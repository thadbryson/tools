{
    "name":              "thadbryson/tools",
    "description":       "Basic PHP programming helper classes. Things for strings, arrays, etc.",
    "license":           "MIT",
    "minimum-stability": "dev",
    "prefer-stable":     true,
    "require":           {
        "php":                            "^7.4 || ^8.0",
        "ext-intl":                       "*",
        "ext-json":                       "*",
        "ext-simplexml":                  "*",
        "beberlei/assert":                ">=3.2",
        "danielstjules/stringy":          ">=3",
        "giggsey/libphonenumber-for-php": ">=8.10",
        "guzzlehttp/guzzle":              ">=6",
        "illuminate/database":            "^7.30.3 || ^8.22.1",
        "illuminate/http":                "^7.0 || ^8.0",
        "illuminate/support":             "^7.0 || ^8.0",
        "illuminate/validation":          "^7.0 || ^8.0",
        "ramsey/uuid":                    ">=3",
        "roave/security-advisories":      "dev-latest",
        "twig/twig":                      ">=2"
    },
    "require-dev":       {
        "codeception/codeception": "4.*",
        "mikey179/vfsstream":      "1.*",
        "mockery/mockery":         "1.*"
    },
    "autoload":          {
        "psr-4": {
            "Tool\\": "src/"
        }
    },
    "autoload-dev":      {
        "psr-4": {
            "_generated\\":     "tests/_support/_generated/",
            "Helper\\":         "tests/_support/Helper/",
            "Tests\\Support\\": "tests/_support/",
            "Tests\\Unit\\":    "tests/unit/"
        }
    },
    "extra":             {
        "branch-alias": {
            "dev-community": "1.0-dev"
        }
    }
}
